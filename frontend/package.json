{
  "name": "wedx-chat-completion-ui",
  "private": true,
  "version": "0.20.6",
  "type": "module",
  "description": "WeDX Chat Completion UI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/motojinc25/wedx-chat-completion.git"
  },
  "homepage": "https://github.com/motojinc25/wedx-chat-completion",
  "bugs": {
    "url": "https://github.com/motojinc25/wedx-chat-completion/issues"
  },
  "scripts": {
    "dev": "vite --host",
    "clean": "shx rm -rf dist ../backend/dist",
    "build:info": "node make-build-info.mjs",
    "build": "pnpm run build:info && pnpm run clean && tsc -b && vite build && pnpm run copy-to-backend && pnpm run postbuild",
    "copy-to-backend": "shx mkdir -p ../backend/dist && shx cp -r dist/* ../backend/dist/",
    "postbuild": "shx cp public/build-info.json ../backend/build-info.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:biome": "biome lint src/",
    "lint:biome:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:write": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "preview": "vite preview",
    "dev:backend": "cd ../backend && uv run python main.py",
    "dev:full": "node dev-full.mjs || true",
    "dev:full:concurrent": "concurrently \"pnpm run dev:backend\" \"pnpm run dev\" --names \"B,F\" --prefix-colors \"blue,green\"",
    "lint:backend": "cd ../backend && uv run ruff check .",
    "lint:backend:fix": "cd ../backend && uv run ruff check --fix .",
    "format:backend": "cd ../backend && uv run ruff format .",
    "check:backend": "cd ../backend && uv run ruff check . && uv run ruff format --check .",
    "check:backend:fix": "cd ../backend && uv run ruff check --fix . && uv run ruff format .",
    "test:backend": "cd ../backend && uv run pytest"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.18.0",
    "@azure/msal-react": "^3.0.16",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/table-core": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.525.0",
    "monaco-editor": "^0.52.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.2",
    "rehype-highlight": "^7.0.2",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^1.7.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@eslint/js": "^9.32.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "shx": "^0.4.0",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  }
}
